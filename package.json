{
  "scripts": {
    "dev": "node --inspect index",
    "start": "cross-env NODE_ENV=production node index",
    "build": "rm -rf dist && mkdir dist && npm run build:client && npm run build:server",
    "build:win": "rmdir /Q /S dist & mkdir dist & npm run build:client & npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter .",
    "lint:fix": "eslint --fix --ext .js,.vue -f ./node_modules/eslint-friendly-formatter .",
    "test": "cross-env NODE_ENV=test nyc --require @babel/register --require @babel/polyfill mocha ./test --recursive --timeout 999999 --colors --exit",
    "cover": "nyc report --reporter=text-lcov | coveralls && rm -rf ./coverage && rm -rf ./.nyc_output",
    "upgrade": "node scripts/upgrade.js"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "acorn": "^6.0.4",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "bufferutil": "^4.0.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "express": "^4.16.4",
    "geoip-lite": "^1.3.3",
    "markdown-it": "^8.4.2",
    "markdown-it-mathjax": "^2.0.0",
    "md5": "^2.2.1",
    "memory-fs": "^0.4.1",
    "mongodb": "^3.1.10",
    "multer": "^1.4.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "regenerator-runtime": "^0.13.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "ua-parser-js": "^0.7.20",
    "utf-8-validate": "^5.0.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.1.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.0.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.10.0",
    "nyc": "^14.1.1",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3"
  }
